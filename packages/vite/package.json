{
  "name": "@pastweb/vite-plugin-css-tools",
  "version": "1.0.0",
  "description": "plugin for Css Utility Modules",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && rimraf doc && rollup --bundleConfigAsCjs --config rollup.config.ts --configPlugin typescript",
    "dev": "rollup --config --bundleConfigAsCjs rollup.config.ts --configPlugin typescript -w",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "tailwind",
    "css",
    "modules",
    "utility"
  ],
  "author": "Domenico Pasto",
  "license": "MIT",
  "peerDependencies": {
    "postcss": "*"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "acorn": "^8.12.1",
    "acorn-walk": "^8.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "ts-jest": "^29.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  }
}
